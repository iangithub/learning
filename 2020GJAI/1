 protected virtual void Dispose(bool disposing)
        {
            if (!disposedValue)
            {
                if (disposing)
                {
                    _frameGrabTimer.Dispose();
                    _timer?.Dispose();
                    _timerMutex.Dispose();
                    _analysisTaskQueue?.Dispose();
                }

                disposedValue = true;
            }
        }

        public void Dispose()
        {
            Dispose(true);
        }
