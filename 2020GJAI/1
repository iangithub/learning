 public class ImageData
    {
        [LoadColumn(0)]
        public string ImagePath;

        [LoadColumn(1)]
        public string Label;
    }

    public class ImagePrediction : ImageData
    {
        public float[] Score;

        /// <summary>
        /// 已預測影像分類標籤的值
        /// </summary>
        public string PredictedLabelValue;
    }
